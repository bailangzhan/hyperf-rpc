{
    "name": "hyperf/db-connection",
    "description": "A hyperf db connection handler for hyperf/database.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "database",
        "connection"
    ],
    "homepage": "https://hyperf.io",
    "support": {
        "docs": "https://hyperf.wiki",
        "issues": "https://github.com/hyperf/hyperf/issues",
        "pull-request": "https://github.com/hyperf/hyperf/pulls",
        "source": "https://github.com/hyperf/hyperf"
    },
    "require": {
        "php": ">=7.2",
        "hyperf/framework": "~2.2.0",
        "hyperf/database": "~2.2.0",
        "hyperf/di": "~2.2.0",
        "hyperf/model-listener": "~2.2.0",
        "hyperf/pool": "~2.2.0",
        "hyperf/utils": "~2.2.0"
    },
    "autoload": {
        "psr-4": {
            "Hyperf\\DbConnection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\DbConnection\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.2-dev"
        },
        "hyperf": {
            "config": "Hyperf\\DbConnection\\ConfigProvider"
        }
    }
}
